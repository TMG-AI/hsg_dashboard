{
  "version": 2,
  "crons": [
    { "path": "/api/collect", "schedule": "0 * * * *" },
    { "path": "/api/congress_collect", "schedule": "0 * * * *" },
    { "path": "/api/newsletter_rss_collect", "schedule": "0 * * * *" },
    { "path": "/api/meltwater_collect", "schedule": "0 13 * * *" }
  ],
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        { "key": "Access-Control-Allow-Origin", "value": "*" },
        { "key": "Access-Control-Allow-Methods", "value": "GET,POST,OPTIONS" }
      ]
    }
  ]
}

